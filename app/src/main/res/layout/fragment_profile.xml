<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    android:orientation="vertical"
    tools:context=".fragments.ProfileFragment">

    <RelativeLayout
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="?android:actionBarSize">

        <ImageView
            android:id="@+id/myProfileBack"
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:clickable="true"
            android:focusable="true"
            android:foreground="?android:selectableItemBackground"
            android:padding="15dp"
            android:src="@drawable/ic_back_24" />

        <TextView
            android:id="@+id/tolbar_back"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_toRightOf="@+id/myProfileBack"
            android:text="Profile"
            android:textColor="@color/colorPrimary"
            android:textSize="16sp"
            android:textStyle="normal" />

    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/toolbar"
        android:orientation="vertical">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="10dp"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:cardBackgroundColor="@android:color/transparent"
                        app:cardCornerRadius="5dp"
                        app:cardElevation="0dp">

                        <RelativeLayout

                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/cntrname"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_centerHorizontal="true"
                                android:layout_centerVertical="true" />

                            <de.hdodenhof.circleimageview.CircleImageView
                                android:id="@+id/userPhoto"
                                android:layout_width="100dp"
                                android:layout_height="100dp"
                                android:layout_centerHorizontal="true"
                                android:layout_marginTop="5dp"
                                android:src="@mipmap/profile"
                                app:civ_border_color="#f12234"
                                app:civ_border_width="1dp" />


                            <ImageView

                                android:id="@+id/camerabtn"
                                android:layout_width="30dp"
                                android:layout_height="30dp"
                                android:layout_centerHorizontal="true"
                                android:layout_marginLeft="25dp"
                                android:layout_marginTop="70dp"
                                android:layout_toRightOf="@+id/cntrname"
                                android:src="@drawable/g_camera" />

                            <TextView
                                android:id="@+id/profileUserName"
                                android:layout_width="wrap_content"
                                android:layout_height="40dp"
                                android:layout_below="@+id/userPhoto"
                                android:layout_centerHorizontal="true"
                                android:layout_gravity="center"
                                android:layout_marginTop="5dp"
                                android:layout_marginBottom="10dp"
                                android:text="user name"
                                android:textColor="@color/colorPrimary"
                                android:textSize="15dp"
                                android:textStyle="bold"
                                android:visibility="invisible" />
                        </RelativeLayout>


                    </androidx.cardview.widget.CardView>


                    <!--// profile Details-->
                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginHorizontal="10dp"
                        android:layout_marginTop="110dp"
                        app:cardCornerRadius="10dp"
                        app:cardUseCompatPadding="true">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="0dp"
                                android:layout_weight="1"
                                android:background="@drawable/header_bg_gradient"
                                android:gravity="center"
                                android:padding="8dp"
                                android:text="User Details"
                                android:textColor="@color/headerColor"
                                android:textStyle="bold"
                                android:typeface="monospace" />

                            <libs.mjn.fieldset.FieldSetView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginHorizontal="15dp"
                                android:layout_marginTop="5dp"
                                app:fsv_borderAlpha="0.75"
                                app:fsv_borderColor="@color/grey"
                                app:fsv_borderRadius="5dp"
                                app:fsv_borderWidth="1.5dp"
                                app:fsv_legend="User Name*"
                                app:fsv_legendColor="@color/grey"
                                app:fsv_legendFont="lobster.ttf"
                                app:fsv_legendPosition="left"
                                app:fsv_legendSize="10sp">

                                <TextView
                                    android:id="@+id/userProfileTvUserName"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@android:color/transparent"
                                    android:maxLines="1"
                                    android:paddingHorizontal="10dp"
                                    android:paddingVertical="3dp"
                                    android:singleLine="true"
                                    android:text=""
                                    android:textColor="@color/browser_actions_title_color"
                                    android:textSize="14sp" />
                            </libs.mjn.fieldset.FieldSetView>

                            <androidx.appcompat.widget.LinearLayoutCompat
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginHorizontal="15dp"
                                android:layout_marginTop="10dp"
                                android:orientation="horizontal">

                                <libs.mjn.fieldset.FieldSetView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="10dp"
                                    android:layout_weight="0.50"
                                    app:fsv_borderAlpha="0.75"
                                    app:fsv_borderColor="@color/grey"
                                    app:fsv_borderRadius="5dp"
                                    app:fsv_borderWidth="1.5dp"
                                    app:fsv_legend="First Name*"
                                    app:fsv_legendColor="@color/grey"
                                    app:fsv_legendFont="lobster.ttf"
                                    app:fsv_legendPosition="left"
                                    app:fsv_legendSize="10sp">

                                    <EditText
                                        android:id="@+id/userProfileTvFirstName"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:background="@android:color/transparent"
                                        android:maxLines="1"
                                        android:paddingHorizontal="10dp"
                                        android:paddingVertical="3dp"
                                        android:singleLine="true"
                                        android:text=""
                                        android:textColor="@color/black"
                                        android:textSize="14sp" />
                                </libs.mjn.fieldset.FieldSetView>

                                <libs.mjn.fieldset.FieldSetView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="10dp"
                                    android:layout_weight="0.50"
                                    app:fsv_borderAlpha="0.75"
                                    app:fsv_borderColor="@color/grey"
                                    app:fsv_borderRadius="5dp"
                                    app:fsv_borderWidth="1.5dp"
                                    app:fsv_legend="Last Name"
                                    app:fsv_legendColor="@color/grey"
                                    app:fsv_legendFont="lobster.ttf"
                                    app:fsv_legendPosition="left"
                                    app:fsv_legendSize="10sp">

                                    <EditText
                                        android:id="@+id/userProfileTvFLastName"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:background="@android:color/transparent"
                                        android:maxLines="1"
                                        android:paddingHorizontal="10dp"
                                        android:paddingVertical="3dp"
                                        android:singleLine="true"
                                        android:text=""
                                        android:textColor="@color/black"
                                        android:textSize="14sp" />
                                </libs.mjn.fieldset.FieldSetView>
                            </androidx.appcompat.widget.LinearLayoutCompat>

                            <libs.mjn.fieldset.FieldSetView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginHorizontal="15dp"
                                android:layout_marginTop="10dp"
                                app:fsv_borderAlpha="0.75"
                                app:fsv_borderColor="@color/grey"
                                app:fsv_borderRadius="5dp"
                                app:fsv_borderWidth="1.5dp"
                                app:fsv_legend="Email ID*"
                                app:fsv_legendColor="@color/grey"
                                app:fsv_legendFont="lobster.ttf"
                                app:fsv_legendPosition="left"
                                app:fsv_legendSize="10sp">

                                <TextView
                                    android:id="@+id/userProfileTvEmailId"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@android:color/transparent"
                                    android:maxLines="1"
                                    android:paddingHorizontal="10dp"
                                    android:paddingVertical="3dp"
                                    android:singleLine="true"
                                    android:text=""
                                    android:textColor="@color/browser_actions_title_color"
                                    android:textSize="14sp" />
                            </libs.mjn.fieldset.FieldSetView>

                            <libs.mjn.fieldset.FieldSetView
                                android:id="@+id/tvprofileProfession"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginHorizontal="15dp"
                                android:layout_marginTop="10dp"
                                app:fsv_borderAlpha="0.75"
                                app:fsv_borderColor="@color/grey"
                                app:fsv_borderRadius="5dp"
                                app:fsv_borderWidth="1.5dp"
                                app:fsv_legend="Profession*"
                                app:fsv_legendColor="@color/grey"
                                app:fsv_legendFont="lobster.ttf"
                                app:fsv_legendPosition="left"
                                app:fsv_legendSize="10sp">

                                <TextView
                                    android:id="@+id/userProfileTvProfessionName"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@android:color/transparent"
                                    android:drawableRight="@drawable/ic_down_arrow"
                                    android:maxLines="1"
                                    android:paddingHorizontal="10dp"
                                    android:paddingVertical="3dp"
                                    android:singleLine="true"
                                    android:text=""
                                    android:hint="Select Profession"
                                    android:textColor="@color/black"
                                    android:textColorHint="@color/greyLight"
                                    android:textSize="14sp" />
                            </libs.mjn.fieldset.FieldSetView>

                            <androidx.appcompat.widget.LinearLayoutCompat
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginHorizontal="15dp"
                                android:layout_marginTop="10dp"
                                android:orientation="horizontal">

                                <libs.mjn.fieldset.FieldSetView
                                    android:id="@+id/fsvCountry"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="5dp"
                                    android:layout_weight="0.50"
                                    app:fsv_borderAlpha="0.75"
                                    app:fsv_borderColor="@color/grey"
                                    app:fsv_borderRadius="5dp"
                                    app:fsv_borderWidth="1.5dp"
                                    app:fsv_legend="Country*"
                                    app:fsv_legendColor="@color/grey"
                                    app:fsv_legendFont="lobster.ttf"
                                    app:fsv_legendPosition="left"
                                    app:fsv_legendSize="12sp">

                                    <androidx.appcompat.widget.LinearLayoutCompat
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:orientation="horizontal">

                                        <TextView
                                            android:id="@+id/tvCountry"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:drawableRight="@drawable/ic_down_arrow"
                                            android:drawablePadding="10dp"
                                            android:hint="Select country"
                                            android:paddingHorizontal="10dp"
                                            android:paddingVertical="3dp"
                                            android:singleLine="true"
                                            android:textColor="@color/black"
                                            android:textColorHint="@color/greyLight" />

                                    </androidx.appcompat.widget.LinearLayoutCompat>

                                </libs.mjn.fieldset.FieldSetView>

                                <libs.mjn.fieldset.FieldSetView
                                    android:id="@+id/fsvState"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="5dp"
                                    android:layout_weight="0.50"
                                    app:fsv_borderAlpha="0.75"
                                    app:fsv_borderColor="@color/grey"
                                    app:fsv_borderRadius="5dp"
                                    app:fsv_borderWidth="1.5dp"
                                    app:fsv_legend="State*"
                                    app:fsv_legendColor="@color/grey"
                                    app:fsv_legendFont="lobster.ttf"
                                    app:fsv_legendPosition="left"
                                    app:fsv_legendSize="12sp">

                                    <androidx.appcompat.widget.LinearLayoutCompat
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:orientation="horizontal">

                                        <TextView
                                            android:id="@+id/tvState"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:drawableRight="@drawable/ic_down_arrow"
                                            android:drawablePadding="10dp"
                                            android:hint="Select state"
                                            android:paddingHorizontal="10dp"
                                            android:paddingVertical="3dp"
                                            android:singleLine="true"
                                            android:textColor="@color/black"
                                            android:textColorHint="@color/greyLight" />

                                    </androidx.appcompat.widget.LinearLayoutCompat>

                                </libs.mjn.fieldset.FieldSetView>
                            </androidx.appcompat.widget.LinearLayoutCompat>

                            <androidx.appcompat.widget.LinearLayoutCompat
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginHorizontal="15dp"
                                android:layout_marginTop="10dp"
                                android:orientation="horizontal">

                                <libs.mjn.fieldset.FieldSetView

                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="5dp"
                                    android:layout_weight="0.50"
                                    app:fsv_borderAlpha="0.75"
                                    app:fsv_borderColor="@color/grey"
                                    app:fsv_borderRadius="5dp"
                                    app:fsv_borderWidth="1.5dp"
                                    app:fsv_legend="City*"
                                    app:fsv_legendColor="@color/grey"
                                    app:fsv_legendFont="lobster.ttf"
                                    app:fsv_legendPosition="left"
                                    app:fsv_legendSize="12sp">

                                    <androidx.appcompat.widget.LinearLayoutCompat
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:orientation="horizontal">

                                        <EditText
                                            android:id="@+id/etCity"
                                            android:layout_width="0dp"
                                            android:layout_height="wrap_content"
                                            android:layout_weight="0.75"
                                            android:background="@null"
                                            android:drawablePadding="10dp"
                                            android:hint="Enter city"
                                            android:inputType="text"
                                            android:paddingHorizontal="10dp"
                                            android:paddingVertical="3dp"
                                            android:singleLine="true"
                                            android:textColorHint="@color/greyLight"
                                            android:textSize="14sp" />

                                        <ImageView
                                            android:id="@+id/ivSelectCity"
                                            android:layout_width="0dp"
                                            android:layout_height="match_parent"
                                            android:layout_marginEnd="2dp"
                                            android:layout_weight="0.18"
                                            android:paddingHorizontal="9dp"
                                            android:src="@drawable/ic_down_arrow"
                                            android:visibility="visible" />

                                    </androidx.appcompat.widget.LinearLayoutCompat>

                                </libs.mjn.fieldset.FieldSetView>

                                <libs.mjn.fieldset.FieldSetView

                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="5dp"
                                    android:layout_weight="0.50"
                                    app:fsv_borderAlpha="0.75"
                                    app:fsv_borderColor="@color/grey"
                                    app:fsv_borderRadius="5dp"
                                    app:fsv_borderWidth="1.5dp"
                                    app:fsv_legend="Zip Code*"
                                    app:fsv_legendColor="@color/grey"
                                    app:fsv_legendFont="lobster.ttf"
                                    app:fsv_legendPosition="left"
                                    app:fsv_legendSize="12sp">

                                    <androidx.appcompat.widget.LinearLayoutCompat
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:orientation="horizontal">

                                        <EditText
                                            android:id="@+id/etZipCode"
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:background="@null"
                                            android:hint="Zip code"
                                            android:inputType="number"
                                            android:paddingHorizontal="10dp"
                                            android:paddingVertical="3dp"
                                            android:singleLine="true"
                                            android:textColorHint="@color/greyLight"
                                            android:textSize="14sp" />

                                    </androidx.appcompat.widget.LinearLayoutCompat>

                                </libs.mjn.fieldset.FieldSetView>
                            </androidx.appcompat.widget.LinearLayoutCompat>

                            <libs.mjn.fieldset.FieldSetView
                                android:id="@+id/tvAddress1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginHorizontal="15dp"
                                android:layout_marginTop="10dp"
                                app:fsv_borderAlpha="0.75"
                                app:fsv_borderColor="@color/grey"
                                app:fsv_borderRadius="5dp"
                                app:fsv_borderWidth="1.5dp"
                                app:fsv_legend="Address*"
                                app:fsv_legendColor="@color/grey"
                                app:fsv_legendFont="lobster.ttf"
                                app:fsv_legendPosition="left"
                                app:fsv_legendSize="10sp">

                                <EditText
                                    android:id="@+id/etAddress"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@android:color/transparent"
                                    android:gravity="top|left"
                                    android:hint="Enter address"
                                    android:inputType="textMultiLine"
                                    android:minHeight="50dp"
                                    android:paddingHorizontal="10dp"
                                    android:paddingVertical="3dp"
                                    android:text=""
                                    android:textColor="@color/black"
                                    android:textColorHint="@color/greyLight"
                                    android:textSize="14sp" />
                            </libs.mjn.fieldset.FieldSetView>

                            <!--<libs.mjn.fieldset.FieldSetView
                                android:id="@+id/tvAddress2"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="30dp"
                                android:layout_marginTop="10dp"
                                android:layout_marginEnd="30dp"
                                app:fsv_borderAlpha="0.75"
                                app:fsv_borderColor="#000000"
                                app:fsv_borderRadius="5dp"
                                app:fsv_borderWidth="1dp"
                                app:fsv_legend="Address (optional)"
                                app:fsv_legendColor="@color/grey"
                                app:fsv_legendFont="lobster.ttf"
                                app:fsv_legendPosition="left"
                                app:fsv_legendSize="10sp">

                                <EditText
                                    android:id="@+id/etAddress2"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@android:color/transparent"
                                    android:hint="Enter address"
                                    android:inputType="textPostalAddress"
                                    android:maxLines="1"
                                    android:paddingHorizontal="10dp"
                                    android:paddingVertical="3dp"
                                    android:text=""
                                    android:textColor="@color/black"
                                    android:textColorHint="@color/greyLight"
                                    android:textSize="14sp" />
                            </libs.mjn.fieldset.FieldSetView>-->

                            <androidx.appcompat.widget.AppCompatButton
                                android:id="@+id/profileUserDetailsUpdateBtn"
                                android:layout_width="100dp"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginVertical="15dp"
                                android:text="@string/common_google_play_services_update_button"
                                android:textColor="@color/white" />


                        </LinearLayout>

                    </androidx.cardview.widget.CardView>

                </RelativeLayout>
                <!--// phone Details-->
                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    app:cardCornerRadius="10dp"
                    app:cardUseCompatPadding="true">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">


                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:background="@drawable/header_bg_gradient"
                            android:gravity="center"
                            android:padding="8dp"
                            android:text="Update Mobile Number"
                            android:textColor="@color/headerColor"
                            android:textStyle="bold"
                            android:typeface="monospace" />

                        <libs.mjn.fieldset.FieldSetView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginHorizontal="15dp"
                            android:layout_marginTop="10dp"
                            app:fsv_borderAlpha="0.75"
                            app:fsv_borderColor="@color/grey"
                            app:fsv_borderRadius="5dp"
                            app:fsv_borderWidth="1.5dp"
                            app:fsv_legend="Mobile Number"
                            app:fsv_legendColor="@color/grey"
                            app:fsv_legendFont="lobster.ttf"
                            app:fsv_legendPosition="left"
                            app:fsv_legendSize="10sp">

                            <androidx.appcompat.widget.LinearLayoutCompat
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:orientation="horizontal">

                                <com.hbb20.CountryCodePicker
                                    android:id="@+id/ccpicker"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    app:defaultCode="91"
                                    app:defaultNameCode="IN"
                                    app:textSize="12sp" />

                                <EditText
                                    android:id="@+id/userProfileTvUserPhone"
                                    android:layout_width="match_parent"
                                    android:layout_height="15dp"
                                    android:layout_gravity="center"
                                    android:background="@android:color/transparent"
                                    android:inputType="phone"
                                    android:maxLines="1"
                                    android:paddingStart="10dp"
                                    android:paddingEnd="0dp"
                                    android:singleLine="true"
                                    android:text=""
                                    android:textColor="@color/black"
                                    android:textSize="14sp" />
                            </androidx.appcompat.widget.LinearLayoutCompat>
                        </libs.mjn.fieldset.FieldSetView>


                        <androidx.appcompat.widget.AppCompatButton
                            android:id="@+id/profileUpdateNumberBtn"
                            android:layout_width="100dp"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:layout_gravity="center"
                            android:layout_marginTop="10dp"
                            android:layout_marginBottom="10dp"
                            android:background="@drawable/button_gradent"
                            android:text="update"
                            android:textColor="@color/white" />

                    </LinearLayout>


                </androidx.cardview.widget.CardView>

                <!--//change password-->
                <androidx.cardview.widget.CardView
                    android:id="@+id/changePassWordCardView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    app:cardCornerRadius="10dp"
                    app:cardUseCompatPadding="true">

                    <LinearLayout

                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">


                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="0dp"
                            android:layout_weight="1"
                            android:background="@drawable/header_bg_gradient"
                            android:gravity="center"
                            android:padding="8dp"
                            android:text="Change Password"
                            android:textColor="@color/headerColor"
                            android:textStyle="bold"
                            android:typeface="monospace" />

                        <libs.mjn.fieldset.FieldSetView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginHorizontal="15dp"
                            android:layout_marginTop="10dp"
                            app:fsv_borderAlpha="0.75"
                            app:fsv_borderColor="@color/grey"
                            app:fsv_borderRadius="5dp"
                            app:fsv_borderWidth="1.5dp"
                            app:fsv_legend="Current Password"
                            app:fsv_legendColor="@color/grey"
                            app:fsv_legendFont="lobster.ttf"
